#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    string a;
    int num;
    cin>>num;
    for(int i =0;i<num;i++)
    {
        cin>>a;
        for(int i = 0;i<a.length();i+=2)
        {
            cout<<a[i];            
        }
        cout<<" ";
        for(int i=1;i<a.length();i+=2)
        {
            cout<<a[i];            
        }
        cout<<" "<<endl;
        
         
    }
    return 0;
}
