#include <bits/stdc++.h>

using namespace std;

int factorial(int n) {
    int fact;
   if(n<=1){
    return 1;
    }
      else{fact = n * factorial(n-1);
          }
   
        return fact;
}

int main() {
    int n;
    cin >> n;
    int result = factorial(n);
    cout << result << endl;
    return 0;
}
